      "modified_at": "2019-01-15T15:53:59.000Z",
      "location": {
        "lat": 60.14891815185547,
        "lon": 24.984798431396484,
        "address": {
          "street_address": "Suomenlinna",
          "postal_code": "00190",
          "locality": "Helsinki"
